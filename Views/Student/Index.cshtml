@model IEnumerable<WebApplication15.Models.Student>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

@{ if (ViewBag.msg != null)
    {
         @ViewBag.msg
    }
}
<table class="table">
    <tr>
        <th> ID </th>
        <th> Name </th>
        <th> Batch Code </th>
        <th> Marks </th>
    </tr>
    @{ 
        foreach(var temp in Model)
        {

           <tr>
               <td> @temp.Id</td>
                <td> @temp.Name </td>
            <td> @temp.BatchCode</td>
            <td> @temp.Marks</td>
           </tr>
        }}
</table>

